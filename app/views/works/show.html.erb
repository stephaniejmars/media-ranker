<section>
  <p>This is the SHOW page for:</p><%= @work.title %> </p>
  <p>Created by: <%= @work.creator %> </p>
  <p>Published: <%= @work.publication_year %> </p>
  <p><%= @work.description %></p>
</section>
<section>
    <span><%= button_to "Back to media ranks", root_path, :class => "btn btn-primary"%></span>
    <span><%= button_to "Edit", edit_work_path, :class => "btn btn-primary"%></span>
    <span><%= button_to "Upvote (Not Built)", root_path, :class => "btn btn-primary"%></span>
    <span>
    <%= button_to "Delete", work_path(@work.id), 
    method: :delete, 
    data: {confirm: "Are you sure you want to delete this work?"},
    :class => "btn btn-danger" %>
    </span>
  
</section>
