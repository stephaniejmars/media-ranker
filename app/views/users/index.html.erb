
<section>
  <h3>List of Users</h3>
  <table class="table">
    <thead>
      <tr>
        <th>Username</th>
        <th>Votes</th>
        <th>Joined</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <% if user.user_name != ""%>
          <td><%= link_to user.user_name, user_path(user.id)%></td>
          <td><%= user.votes.count %></td>
          <td><%= user.created_at.strftime("%B %d, %Y")%></td>
          <%end%>
        </tr>
      <% end %>
    </tbody>
  </table>
</section>